#include <stdio.h>
int main()
{
int t;
int n,i,count,min;
scanf("%d",&t);
while (t--)
{
scanf("%d",&n);
long sp1;
        for (i=0;i<n;i++)
{
            scanf("%ld",&sp1);
            if (i == 0)
            {
                min=sp1;
                count=1;
            }
else
            {
                if (sp1 < min)
                {
                    count++;
                    min=sp1;
                }
            }
}
printf("%d\n",count);
}
return 0;
}
